<%= form_for([@business, @business.reviews.build]) do |f| %>
  <p>
    <%= f.label :reviewer %><br>
    <%= f.text_field :reviewer %>
  </p>
  <p>
    <%= f.label :review %><br>
    <%= f.text_area :review %>
  </p>
  <p>
    <%= f.label :rating %><br>
    <%= f.number_field :rating %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>